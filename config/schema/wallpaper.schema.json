{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "wallpaper": {
      "$id": "/properties/wallpaper",
      "type": "object",
      "properties": {
        "downloadsPath": {
          "$id": "/properties/wallpaper/properties/downloadPath",
          "type": "string",
          "examples": [
            "/home/USERNAME/tmp"
          ]
        },
        "defaultUrl": {
          "$id": "/properties/wallpaper/properties/defaultUrl",
          "type": "string",
          "examples": [
            "www.reddit.com/r/wallpapers.json"
          ]
        },
        "historyFilepath": {
          "$id": "/properties/wallpaper/properties/defaultUrl",
          "type": "string",
          "examples": [
            "/tmp/.history"
          ]
        },
        "imagesExtension": {
          "$id": "/properties/wallpaper/properties/imagesExtension",
          "type": "string",
          "examples": [
            "/tmp/.history"
          ]
        }
      },
        "required": ["defaultUrl","downloadsPath","historyFilepath","imagesExtension"]
    }
  }
}
